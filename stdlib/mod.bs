#ifndef MOD_BS
#define MOD_BS

#include <math/div_base.bs>

mod(x, y)
{
``
    lda .x
    sta div_base_.x
    lda .x + 1
    sta div_base_.x + 1
    lda .y
    sta div_base_.y
    lda .y + 1
    sta div_base_.y + 1
    call div_base_
    lda div_base_.remainder
    sta _return
    lda div_base_.remainder + 1
    sta _return + 1
``
}

#endif

