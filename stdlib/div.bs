#ifndef DIV_BS
#define DIV_BS

#include <math/div_base.bs>

div(x, y)
{
``
    lda .x
    sta div_base.x
    lda .x + 1
    sta div_base.x + 1
    lda .y
    sta div_base.y
    lda .y + 1
    sta div_base.y + 1
    phc
    jmp div_base
    lda div_base.x
    sta _return
    lda div_base.x + 1
    sta _return + 1
``
}

#endif

